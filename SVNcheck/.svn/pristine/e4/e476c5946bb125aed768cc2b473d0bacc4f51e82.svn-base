<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    style="@style/AppSystemBar"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".ui.view.activity.MainActivity">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="#327ECA">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="50dp"
            android:layout_centerInParent="true"
            android:gravity="center"
            android:text="天裕汽车一体销售平台"
            android:textColor="#ffffff"
            android:textSize="18sp" />

    </RelativeLayout>

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="150dp"
        android:scaleType="centerCrop"
        android:src="@mipmap/main_banner" />


    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:background="@color/appBlue">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_centerInParent="true"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/activity_cpinfo_user_icon"
                        android:layout_width="50dp"
                        android:layout_height="match_parent"
                        android:layout_centerInParent="true"
                        android:onClick="listOnclick"
                        android:scaleType="fitXY"
                        android:src="@mipmap/user" />

                    <TextView
                        android:id="@+id/activity_main_tv_zhanghao"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:textColor="@color/white"
                        android:textSize="18sp" />

                </LinearLayout>


            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/activity_main_rl_ptcustomers"
                android:layout_width="match_parent"
                android:layout_height="100dp"
                android:onClick="listOnclick"
                android:visibility="gone">

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="100dp"
                    android:scaleType="centerCrop"
                    android:src="@drawable/pt_customer_selector" />

                <LinearLayout
                    android:layout_width="90dp"
                    android:layout_height="match_parent"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:id="@+id/activity_main_ll_ptcustomers_total_today"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_marginBottom="1dp"
                        android:layout_marginRight="1dp"
                        android:layout_marginTop="1dp"
                        android:layout_weight="1"
                        android:background="@drawable/blueonclick_selecter"
                        android:clickable="true"
                        android:onClick="listOnclick"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="50dp"
                            android:gravity="bottom|center_horizontal"
                            android:paddingBottom="5dp"
                            android:text="今日"
                            android:textColor="@color/white"
                            android:textSize="13sp" />

                        <TextView
                            android:id="@+id/activity_main_tv_ptcustomers_total_today"
                            android:layout_width="match_parent"
                            android:layout_height="50dp"
                            android:ellipsize="end"
                            android:gravity="top|center_horizontal"
                            android:paddingTop="5dp"
                            android:singleLine="true"
                            android:textColor="@color/white"
                            android:textSize="18sp" />


                    </LinearLayout>


                    <LinearLayout
                        android:id="@+id/activity_main_ll_ptcustomers_total_history"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_marginBottom="1dp"
                        android:layout_marginRight="1dp"
                        android:layout_marginTop="1dp"
                        android:layout_weight="1"
                        android:background="@drawable/orangeonclick_selector"
                        android:clickable="true"
                        android:onClick="listOnclick"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="50dp"
                            android:gravity="bottom|center_horizontal"
                            android:paddingBottom="5dp"
                            android:text="逾期"
                            android:textColor="@color/white"
                            android:textSize="13sp" />

                        <TextView
                            android:id="@+id/activity_main_tv_ptcustomers_total_history"
                            android:layout_width="match_parent"
                            android:layout_height="50dp"
                            android:ellipsize="end"
                            android:gravity="top|center_horizontal"
                            android:paddingTop="5dp"
                            android:singleLine="true"
                            android:textColor="@color/white"
                            android:textSize="18sp" />


                    </LinearLayout>
                </LinearLayout>


            </RelativeLayout>


            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="#aaaaaa" />


            <RelativeLayout
                android:id="@+id/activity_main_rl_clue"
                android:layout_width="match_parent"
                android:layout_height="100dp"
                android:clickable="true"
                android:onClick="listOnclick"
                android:visibility="gone">

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="100dp"
                    android:scaleType="centerCrop"
                    android:src="@drawable/clue_selector" />


                <LinearLayout
                    android:layout_width="90dp"
                    android:layout_height="match_parent"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:id="@+id/activity_main_ll_clue_total_today"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_marginBottom="1dp"
                        android:layout_marginRight="1dp"
                        android:layout_marginTop="1dp"
                        android:layout_weight="1"
                        android:background="@drawable/blueonclick_selecter"
                        android:clickable="true"
                        android:onClick="listOnclick"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="50dp"
                            android:gravity="bottom|center_horizontal"
                            android:paddingBottom="5dp"
                            android:text="今日"
                            android:textColor="@color/white"
                            android:textSize="13sp" />

                        <TextView
                            android:id="@+id/activity_main_tv_clue_total_today"
                            android:layout_width="match_parent"
                            android:layout_height="50dp"
                            android:ellipsize="end"
                            android:gravity="top|center_horizontal"
                            android:paddingTop="5dp"
                            android:singleLine="true"
                            android:textColor="@color/white"
                            android:textSize="18sp" />


                    </LinearLayout>


                    <LinearLayout
                        android:id="@+id/activity_main_ll_clue_total_history"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_marginBottom="1dp"
                        android:layout_marginRight="1dp"
                        android:layout_marginTop="1dp"
                        android:layout_weight="1"
                        android:background="@drawable/orangeonclick_selector"
                        android:clickable="true"
                        android:onClick="listOnclick"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="50dp"
                            android:gravity="bottom|center_horizontal"
                            android:paddingBottom="5dp"
                            android:text="逾期"
                            android:textColor="@color/white"
                            android:textSize="13sp" />

                        <TextView
                            android:id="@+id/activity_main_tv_clue_total_history"
                            android:layout_width="match_parent"
                            android:layout_height="50dp"
                            android:ellipsize="end"
                            android:gravity="top|center_horizontal"
                            android:paddingTop="5dp"
                            android:singleLine="true"
                            android:textColor="@color/white"
                            android:textSize="18sp" />


                    </LinearLayout>
                </LinearLayout>

            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="#aaaaaa" />

            <RelativeLayout
                android:id="@+id/activity_main_rl_cpcustomers"
                android:layout_width="match_parent"
                android:layout_height="100dp"
                android:clickable="true"
