                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_marginBottom="1dp"
                        android:layout_marginRight="1dp"
                        android:layout_marginTop="1dp"
                        android:layout_weight="1"
                        android:background="@drawable/blueonclick_selecter"
                        android:clickable="true"
                        android:onClick="listOnclick"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="50dp"
                            android:gravity="bottom|center_horizontal"
                            android:paddingBottom="5dp"
                            android:text="今日"
                            android:textColor="@color/white"
                            android:textSize="13sp" />

                        <TextView
                            android:id="@+id/activity_main_tv_fpcustomers_total_today"
                            android:layout_width="match_parent"
                            android:layout_height="50dp"
                            android:ellipsize="end"
                            android:gravity="top|center_horizontal"
                            android:paddingTop="5dp"
                            android:singleLine="true"
                            android:textColor="@color/white"
                            android:textSize="18sp" />


                    </LinearLayout>


                    <LinearLayout
                        android:id="@+id/activity_main_ll_fpcustomers_total_history"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_marginBottom="1dp"
                        android:layout_marginRight="1dp"
                        android:layout_marginTop="1dp"
                        android:layout_weight="1"
                        android:background="@drawable/orangeonclick_selector"
                        android:clickable="true"
                        android:onClick="listOnclick"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="50dp"
                            android:gravity="bottom|center_horizontal"
                            android:paddingBottom="5dp"
                            android:text="逾期"
                            android:textColor="@color/white"
                            android:textSize="13sp" />

                        <TextView
                            android:id="@+id/activity_main_tv_fpcustomers_total_history"
                            android:layout_width="match_parent"
                            android:layout_height="50dp"
                            android:ellipsize="end"
                            android:gravity="top|center_horizontal"
                            android:paddingTop="5dp"
                            android:singleLine="true"
                            android:textColor="@color/white"
                            android:textSize="18sp" />
                    </LinearLayout>
                </LinearLayout>


            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="#aaaaaa" />

