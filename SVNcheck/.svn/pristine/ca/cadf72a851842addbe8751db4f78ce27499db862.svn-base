<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="#327ECA">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_centerInParent="true"
            android:gravity="center"
            android:text="拨出成功"
            android:textColor="@color/white"
            android:textSize="@dimen/activtiy_header_text_size" />

        <ImageView
            android:id="@+id/activity_calloutsuccess_save"
            android:layout_width="50dp"
            android:layout_height="match_parent"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:scaleType="fitXY"
            android:src="@drawable/save_selector" />
    </RelativeLayout>
    <ScrollView
        android:scrollbars="none"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">



        <View
            android:background="#dddddd"
            android:layout_width="match_parent"
            android:layout_height="8dp"/>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="50dp">
            <TextView
                android:layout_centerVertical="true"
                android:layout_marginLeft="10dp"
                android:text="录音"
                android:textSize="16sp"
                android:layout_alignParentLeft="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <ImageView
                android:id="@+id/activity_calloutsuccess_iv_playIcon"
                android:src="@mipmap/playstart"
                android:scaleType="centerInside"
                android:layout_marginRight="10dp"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                android:layout_width="30dp"
                android:layout_height="30dp" />
            <TextView
                android:id="@+id/activity_calloutsuccess_tv_duration"
                android:layout_toLeftOf="@id/activity_calloutsuccess_iv_playIcon"
                android:layout_centerVertical="true"
                android:layout_marginRight="10dp"
                android:text="0:10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
        </RelativeLayout>

        <View
            android:background="#dddddd"
            android:layout_width="match_parent"
            android:layout_height="8dp"/>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <RelativeLayout
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="wrap_content">

                <TextView
                    android:text="再次邀约"
                    android:textSize="16sp"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="20dp"
                    android:layout_marginBottom="20dp"
                    android:layout_alignParentLeft="true"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />

                <CheckBox
                    android:id="@+id/activity_calloutsuccess_cb_invite"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:button="@drawable/pingpaitexin_selector"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />

            </RelativeLayout>

            <View
                android:background="#ddd"
                android:layout_width="2dp"
                android:layout_height="match_parent"/>
            <RelativeLayout
                android:background="#ddd"
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="match_parent">

                <EditText
                    android:id="@+id/activity_calloutsuccess_et_invitetime"
                    android:hint="再次邀约日期"
                    android:layout_marginLeft="10dp"
                    android:background="#0000ffff"
                    android:textSize="16sp"
                    android:focusable="false"
                    android:layout_centerVertical="true"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />


            </RelativeLayout>
        </LinearLayout>

        <View
            android:background="#ddd"
            android:layout_width="match_parent"
            android:layout_height="8dp"/>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:text="已保养"
                android:layout_marginLeft="10dp"
                android:textSize="16sp"
                android:layout_marginTop="20dp"
                android:layout_marginBottom="20dp"
                android:layout_centerVertical="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />

            <CheckBox
                android:id="@+id/activity_calloutsuccess_cb_upkeep"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:button="@drawable/pingpaitexin_selector"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />

        </RelativeLayout>
        <View
            android:id="@+id/activity_calloutsuccess_v_upkepptime"
            android:background="#ddd"
            android:layout_width="match_parent"
            android:layout_height="8dp"/>

        <LinearLayout
            android:id="@+id/activity_calloutsuccess_ll_upkepptime"
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <RelativeLayout
                android:layout_weight="1"
                android:layout_width="0dp"
                android:background="#ddd"
                android:layout_height="wrap_content">

                <EditText
                    android:id="@+id/activity_calloutsuccess_et_upkeeptime"
                    android:hint="已保养日期"
                    android:textSize="16sp"
                    android:focusable="false"
                    android:background="#00ffffff"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="10dp"
                    android:layout_marginBottom="20dp"
                    android:layout_marginTop="20dp"
                    android:layout_alignParentLeft="true"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />

            </RelativeLayout>

            <View
                android:background="#ddd"
                android:layout_width="2dp"
                android:layout_height="match_parent"/>
            <RelativeLayout
                android:layout_weight="1"
                android:layout_width="0dp"
                android:background="#ddd"
                android:layout_height="match_parent">

                <EditText
                    android:id="@+id/activity_calloutsucces_et_mil"
                    android:hint="已保养里程"
                    android:focusable="false"
                    android:inputType="number"
                    android:layout_marginLeft="10dp"
                    android:textSize="16sp"
                    android:background="#00ffffff"
                    android:layout_centerVertical="true"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
            </RelativeLayout>
        </LinearLayout>

        <View
            android:background="#ddd"
            android:layout_width="match_parent"
            android:layout_height="8dp"/>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <RelativeLayout
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="wrap_content">

                <TextView
                    android:text="流失客户"
                    android:textSize="16sp"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="20dp"
                    android:layout_marginBottom="20dp"
                    android:layout_alignParentLeft="true"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />

                <CheckBox
                    android:id="@+id/activity_calloutsucces_cb_lostcusotmer"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"
                    android:button="@drawable/pingpaitexin_selector"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />

            </RelativeLayout>

            <View
                android:background="#ddd"
                android:layout_width="2dp"
                android:layout_height="match_parent"/>
            <RelativeLayout
                android:layout_weight="1"
                android:layout_width="0dp"
                android:background="#ddd"
                android:layout_height="match_parent">


                <EditText
                    android:id="@+id/activity_calloutsucces_et_lostreason"
                    android:hint="流失原因"
                    android:focusable="false"
                    android:textSize="16sp"
                    android:layout_marginLeft="10dp"
                    android:background="#00ffffff"
                    android:layout_centerVertical="true"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />

            </RelativeLayout>
        </LinearLayout>


        <View
            android:background="#ddd"
            android:layout_width="match_parent"
            android:layout_height="8dp"/>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <RelativeLayout
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="wrap_content">

                <TextView
                    android:text="成功邀约"
                    android:textSize="16sp"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="20dp"
                    android:layout_marginBottom="20dp"
                    android:layout_alignParentLeft="true"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />

                <CheckBox
                    android:id="@+id/activity_calloutsuccess_cb_invitesuccess"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"
                    android:layout_width="wrap_content"
                    android:button="@drawable/pingpaitexin_selector"
                    android:layout_height="wrap_content" />

            </RelativeLayout>

            <View
                android:background="#ddd"
                android:layout_width="2dp"
                android:layout_height="match_parent"/>
            <RelativeLayout
                android:layout_weight="1"
                android:background="#ddd"
                android:layout_width="0dp"
                android:layout_height="match_parent">


                <TextView
                    android:id="@+id/activity_calloutsucces_tv_comeintime"
                    android:hint="邀约进厂时间"
                    android:focusable="false"
                    android:layout_marginLeft="10dp"
                    android:textSize="16sp"
                    android:background="#00ffffff"
                    android:layout_centerVertical="true"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />

            </RelativeLayout>
        </LinearLayout>

        <View
            android:background="#ddd"
            android:layout_width="match_parent"
            android:layout_height="8dp"/>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <RelativeLayout
                android:layout_weight="1"
                android:layout_width="0dp"
                android:background="#ddd"
                android:layout_height="wrap_content">

                <TextView
                    android:id="@+id/activity_calloutsuccess_tv_lastcometime"
                    android:hint="最迟进厂时间"
                    android:focusable="false"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="10dp"
                    android:textSize="16sp"
                    android:layout_marginTop="20dp"
                    android:layout_marginBottom="20dp"
                    android:background="#00ffffff"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />

            </RelativeLayout>

            <View
                android:background="#ddd"
                android:layout_width="2dp"
                android:layout_height="match_parent"/>
            <RelativeLayout
                android:layout_weight="1"
                android:layout_width="0dp"
                android:background="#ddd"
                android:layout_height="match_parent">

                <AutoCompleteTextView
                    android:id="@+id/activity_calloutsuccess_actv_sa"
                    android:hint="预约服务顾问"
                    android:focusable="false"
                    android:background="#00ffffff"
                    android:completionThreshold="1"
                    android:layout_marginLeft="10dp"
                    android:textSize="16sp"
                    android:layout_centerVertical="true"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />

            </RelativeLayout>
        </LinearLayout>



        <View
            android:background="#ddd"
            android:layout_width="match_parent"
            android:layout_height="8dp"/>

        <LinearLayout
            android:orientation="horizontal"
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <RelativeLayout
                android:layout_weight="1"
                android:layout_width="0dp"
                android:background="#ddd"
                android:layout_height="wrap_content">

                <TextView
                    android:id="@+id/activity_calloutsuccess_tv_timingType"
                    android:hint="定保类型"
                    android:focusable="false"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="10dp"
                    android:textSize="16sp"
                    android:layout_marginTop="20dp"
                    android:layout_marginBottom="20dp"
                    android:background="#00ffffff"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />

            </RelativeLayout>
        </LinearLayout>

        <View
            android:background="#ddd"
            android:layout_width="match_parent"
            android:layout_height="8dp"/>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="100dp">
            <TextView
                android:layout_centerVertical="true"
                android:layout_marginLeft="10dp"
                android:text="洽谈记录"
                android:textSize="16sp"
                android:layout_marginTop="10dp"
                android:layout_width="wrap_content"
                android:layout_height="match_parent" />
            <EditText
                android:id="@+id/activity_calloutsuccesset_talkprocess"
                android:layout_weight="1"
                android:gravity="top"
                android:layout_width="0dp"
                android:background="#fff"
                android:lines="5"
                android:layout_height="match_parent" />
        </LinearLayout>
        <View
            android:background="#ddd"
            android:layout_width="match_parent"
            android:layout_height="16dp"/>

    </LinearLayout>

    </ScrollView>
</LinearLayout>